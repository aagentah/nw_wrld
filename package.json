{
  "name": "nw-wrld",
  "productName": "nw_wrld",
  "version": "0.2.0-beta",
  "description": "nw_wrld is an event-driven sequencer for triggering visuals using web technologies. It enables users to scale up audiovisual compositions for prototyping, demos, exhibitions, and live performances. Users code their own visual modules, then orchestrate them using the project's native UI composer.",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build:main && concurrently -k \"npm run serve\" \"wait-on http://localhost:9000 && electron .\"",
    "serve": "webpack serve --mode development",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "node build-main.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typecheck:watch": "tsc -p tsconfig.json --noEmit --watch",
    "clean": "node -e \"require('fs').rmSync('release',{recursive:true,force:true});require('fs').rmSync('dist/renderer',{recursive:true,force:true});require('fs').rmSync('dist/assets',{recursive:true,force:true});\"",
    "build:renderer": "npm run typecheck && webpack --mode production",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "pack:mac": "npm run build && electron-builder --mac --dir --universal",
    "dist:mac": "npm run build && electron-builder --mac --universal --publish never",
    "pack:mac:split": "npm run build && electron-builder --mac --dir --arm64 && electron-builder --mac --dir --x64",
    "dist:mac:split": "npm run build && electron-builder --mac --arm64 --publish never && electron-builder --mac --x64 --publish never",
    "pack:mac:arm64": "npm run build && electron-builder --mac --dir --arm64",
    "pack:mac:x64": "npm run build && electron-builder --mac --dir --x64",
    "dist:mac:arm64": "npm run build && electron-builder --mac --arm64 --publish never",
    "dist:mac:x64": "npm run build && electron-builder --mac --x64 --publish never",
    "pack:win": "npm run build && electron-builder --win --dir",
    "dist:win": "npm run build && electron-builder --win --publish never"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "midi",
    "visualization",
    "ableton",
    "vj",
    "audiovisual",
    "electron",
    "live-performance",
    "music-visualizer",
    "three-js",
    "p5-js"
  ],
  "author": "https://github.com/aagentah",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/aagentah/nw_wrld.git"
  },
  "bugs": {
    "url": "https://github.com/aagentah/nw_wrld/issues"
  },
  "homepage": "https://github.com/aagentah/nw_wrld#readme",
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@tonejs/midi": "^2.0.28",
    "@tweenjs/tween.js": "^23.1.3",
    "d3": "^7.9.0",
    "immer": "^10.1.1",
    "jotai": "^2.10.0",
    "lodash": "^4.17.21",
    "noisejs": "^2.1.0",
    "osc": "^2.4.4",
    "p5": "^1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "three": "^0.159.0",
    "tone": "^15.1.22",
    "webmidi": "^3.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/three": "^0.160.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.1.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "dmg-builder": "25.1.8",
    "electron": "^39.2.7",
    "electron-builder": "^25.1.8",
    "electron-builder-squirrel-windows": "25.1.8",
    "happy-dom": "^12.10.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.9.3",
    "vitest": "^1.1.0",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "build": {
    "appId": "tech.aagentah.nw_wrld",
    "productName": "nw_wrld",
    "asar": true,
    "npmRebuild": false,
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "index.js",
      "package.json",
      "dist/**",
      "src/index.js",
      "src/preload.js",
      "src/sandboxPreload.js",
      "src/main/**",
      "src/projector/helpers/**",
      "src/projector/views/**",
      "src/dashboard/views/**",
      "src/dashboard/assets/audio/**",
      "src/shared/**",
      "src/assets/**"
    ],
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "build/logo.icns"
    },
    "win": {
      "target": [
        "portable"
      ],
      "icon": "build/logo.ico"
    }
  }
}
